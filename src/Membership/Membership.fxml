      <Button fx:id="MemSbuyButton" layoutX="475.0" layoutY="333.0" minHeight="35.0" minWidth="100.0" mnemonicParsing="false" onAction="#MemSbuyEvent" prefHeight="25.0" prefWidth="100.0" text="Buy">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Label layoutX="156.0" layoutY="214.0" text="\$20.00">
