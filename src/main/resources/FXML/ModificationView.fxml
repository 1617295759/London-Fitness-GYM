               <HBox.margin>
                  <Insets left="100.0" top="5.0" />
               </HBox.margin>
            </Button>
            <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Personal Details">
               <font>
                  <Font name="System Bold" size="41.0" />
               </font>
            </Text>
            <Button fx:id="saveButton" mnemonicParsing="false" onAction="#saveEvent" prefHeight="49.0" prefWidth="108.0" styleClass="modification-button" stylesheets="@../statics/style.css" text="Save">
