                     <HBox.margin>
                        <Insets left="130.0" top="25.0" />
                     </HBox.margin>
                  </Text>
               </children></HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Password">
                     <font>
                        <Font size="32.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="130.0" top="25.0" />
                     </HBox.margin>
                  </Text>
               </children></HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Gender">
                     <font>
                        <Font size="32.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="130.0" top="25.0" />
                     </HBox.margin>
                  </Text>
                  <ComboBox prefHeight="42.0" prefWidth="279.0" promptText="choose your gender" style="-fx-background-color: #ffffff;">
                     <items>
                        <FXCollections fx:factory="observableArrayList">
                           <String fx:value="Male" />
                           <String fx:value="Female" />
                        </FXCollections>
                     </items>
                     <HBox.margin>
                        <Insets left="220.0" right="100.0" top="25.0" />
                     </HBox.margin>
                  </ComboBox>
               </children></HBox>
         </children>
