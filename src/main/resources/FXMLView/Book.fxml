<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.media.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="562.0" prefWidth="789.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="team.gym.FXMLController.Book">
    <children>
        <Separator layoutX="105.0" orientation="VERTICAL" prefHeight="400.0" />
        <Button fx:id="exitButton" layoutX="14.0" layoutY="463.0" mnemonicParsing="false" onAction="#exitEvent" text="Exit" />
        <VBox prefHeight="168.0" prefWidth="105.0">
            <children>
                <ToggleButton fx:id="mineButton" mnemonicParsing="false" onAction="#mineEvent" prefHeight="42.0" prefWidth="105.0" selected="true" text="Mine">
                    <font>
                        <Font size="13.0" />
                    </font>
                    <toggleGroup>
                        <ToggleGroup fx:id="function" />
                    </toggleGroup>
                </ToggleButton>
                <ToggleButton fx:id="videoButton" mnemonicParsing="false" onAction="#videoEvent" prefHeight="42.0" prefWidth="105.0" text="Video" toggleGroup="$function">
                    <font>
                        <Font size="13.0" />
                    </font>
                </ToggleButton>
                <ToggleButton fx:id="bookButton" mnemonicParsing="false" onAction="#bookEvent" prefHeight="42.0" prefWidth="105.0" selected="true" text="Book" toggleGroup="$function">
                    <font>
                        <Font size="13.0" />
                    </font>
                </ToggleButton>
                <ToggleButton fx:id="membershipButton" mnemonicParsing="false" onAction="#membershipEvent" prefHeight="42.0" prefWidth="105.0" text="Membership" toggleGroup="$function">
                    <font>
                        <Font size="13.0" />
                    </font>
                </ToggleButton>
            </children>
        </VBox>
      <TableView fx:id="trainersTable" layoutX="149.0" layoutY="38.0" prefHeight="473.0" prefWidth="577.0">
        <columns>
          <TableColumn fx:id="Name" prefWidth="102.0" text="Coach" />
          <TableColumn fx:id="Gender" prefWidth="109.0" text="gender" />
            <TableColumn fx:id="tele" prefWidth="109.0" text="tele" />
            <TableColumn fx:id="email" prefWidth="109.0" text="e-mail" />
            <TableColumn fx:id="operate" prefWidth="109.0" text="operate" />
        </columns>
      </TableView>
    </children>
</AnchorPane>
